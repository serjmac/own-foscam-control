<% layout('layouts/boilerplate')%>
<div class="col-lg-8 text-center mx-auto">
  <h4 class="d-inlines">Preset list</h4>
  <div class="row col-6 mx-auto">
    <% Object.values(PTZPresets).forEach(function(e) { %>
    <a role="button" href="/gotopreset/<%=e%>" class="btn btn-light btn-lg mt-2 align-content-center">
      <strong><%= e %></strong>
    </a>
    <% }) %>
  </div>
</div>
<script>
  const home = document.getElementById("ptz");
  home.classList.toggle("active");
</script>
